<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.rohitkumarbhamu.fortsapp">

    <!--
         The ACCESS_COARSE/FINE_LOCATION permissions are not required to use
         Google Maps Android API v2, but you must specify either coarse or fine
         location permissions for the 'MyLocation' functionality. 
    -->
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />

    <application
        android:allowBackup="true"
        android:hardwareAccelerated="false"
        android:icon="@mipmap/ic_launcher1"
        android:label="@string/app_name"
        android:largeHeap="true"
        android:roundIcon="@mipmap/ic_launcher1"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".RaigarhActivity"
            android:label="Raigarh Fort"
            android:theme="@style/AppTheme1" />
        <activity
            android:name=".LohagarhActivity"
            android:label="Lohagarh Fort"
            android:theme="@style/AppTheme1" />
        <activity
            android:name=".ShanivarwadaActivity"
            android:label="Shanivarwada Fort"
            android:theme="@style/AppTheme1" />
        <activity
            android:name=".SindhugarhActivity"
            android:label="Sindhugarh Fort"
            android:theme="@style/AppTheme1" />
        <activity
            android:name=".TornaActivity"
            android:label="Torna Fort"
            android:theme="@style/AppTheme1" />
        <activity
            android:name=".panhalaActivity"
            android:label="Panhala Fort"
            android:theme="@style/AppTheme1" />
        <activity
            android:name=".JanjiraActivity"
            android:label="Janjira Fort"
            android:theme="@style/AppTheme1" />
        <activity
            android:name=".DaulatabadActivity"
            android:label="Daulatabad Fort"
            android:theme="@style/AppTheme1" />
        <activity
            android:name=".YashwantgarhActivity"
            android:label="Yashwantgarh Fort"
            android:theme="@style/AppTheme1" />
        <activity
            android:name=".SinhagarhActivity"
            android:label="Sinhagarh Fort"
            android:theme="@style/AppTheme1" />
        <activity
            android:name=".PratapgarhActivity"
            android:label="Pratapgarh Fort"
            android:theme="@style/AppTheme1" />
        <activity
            android:name=".KandharActivity"
            android:label="Kandhar Fort"
            android:theme="@style/AppTheme1" />
        <!--
             The API key for Google Maps-based APIs is defined as a string resource.
             (See the file "res/values/google_maps_api.xml").
             Note that the API key is linked to the encryption key used to sign the APK.
             You need a different API key for each encryption key, including the release key that is used to
             sign the APK for publishing.
             You can define the keys for the debug and release targets in src/debug/ and src/release/. 
        -->
        <meta-data
            android:name="com.google.android.geo.API_KEY"
            android:value="@string/google_maps_key" />

        <activity
            android:name=".MapsActivity"
            android:label="@string/title_activity_maps"></activity>
    </application>

</manifest>